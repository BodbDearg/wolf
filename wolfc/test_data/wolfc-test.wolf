func printHeader()
	print(
		"*** TEST PROGRAM FOR WOLF ***\n"
		"These strings should be folded together!\n"
		"\n"
	)
end

func printNum(int num)
	print("The number is: ")
	print(num)
	print("\n")
end

func main() -> int
	printHeader()
	var a = 1
	a += 2
	printNum(a)
	a -= 2
	printNum(a)
	a *= 3
	printNum(a)
	a /= 3
	printNum(a)
	a++
	printNum(a)
	a--
	printNum(a)
	return 0
end
