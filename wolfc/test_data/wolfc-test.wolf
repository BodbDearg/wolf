func printNum(int num)
	print("The number is: ")
	print(num)
	print("\n")
end

func testModArrayArgs(int[3] numbers)
	printNum(numbers[1])
	numbers[1] = 6
	printNum(numbers[1])
	numbers = [4, 5, 6]
	printNum(numbers[1])
end

func testReturnArray() -> int[3]
	return [4, 5, 6]
end

func main() -> int
	var numbers = [1, 2, 3]
	testModArrayArgs(numbers)
	printNum(numbers[1])
	numbers = testReturnArray()
	printNum(numbers[1])
	return 0
end
